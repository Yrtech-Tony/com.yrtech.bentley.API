@{
    ViewBag.Title = "DocList";
}
<style>
    html {
        overflow: auto;
        overflow-y: hidden;
        overflow-x: hidden;
    }

    .widget {
        margin-bottom: 5px !important;
    }

    .widget-body {
        padding: 5px 5px 1px 5px !important;
    }

    .page-content {
        padding: 5px 5px 1px 5px !important;
    }

    .fixed-table-pagination div.pagination, .fixed-table-pagination .pagination-detail {
        margin-top: 5px !important;
        margin-bottom: 0px !important;
    }
</style>
<input id="ShopId" type="hidden" value="@ViewBag.ShopId" />
<input id="FileId" type="hidden" value="@ViewBag.FileId" />
<div id="page-wrapper">
    <!--BEGIN CONTENT-->
    <div class="page-content">
        <div id="tab-general">
            <div class="row mbl">
                <div class="col-lg-12">
                    <div class="col-md-12">
                        <div id="area-chart-spline" style="width: 100%; height: 300px; display: none;">
                        </div>
                    </div>
                </div>
                <div class="col-lg-12">
                    <!--BEGIN CONTENT-->
                    <div id="main-content">
                        <div class="container-fluid">
                            <p>
                                <a class="btn btn-primary" href="/Document/Index" translate>Back</a>
                                <span style="padding-left:10px;" />
                            </p>
                            <div class="row-fluid">
                                <div class="span12">

                                    <div class="widget">
                                        <div class="widget-title">
                                            <h4><i class="icon-reorder"></i><span translate>Document Submit</span></h4>
                                            <span class="tools">
                                                <a href="javascript:;" class="icon-chevron-down" onclick="ShowDiv()"></a>
                                            </span>
                                        </div>

                                        <div class="widget-body" style="display: block;">
                                            <div class="dataTables_wrapper form-inline" role="grid">
                                                <table id="myPromotionFile" class="table table-striped table-bordered table-hover">
                                                    <thead>
                                                        <tr>
                                                            <th data-filed="ShopCommitFileName" translate>File Name</th>
                                                            <th data-filed="InDateTime" translate>Upload Time</th>
                                                            <th data-filed="ModifyDateTime" translate>Update Time</th>
                                                            <th translate>Edit</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <form action="/Master/FileUpload" enctype="multipart/form-data" id="FileUpload" method="post" style="display:none">
                                <input name="__RequestVerificationToken" type="hidden" value="uVjsH1KVT7z6SlT4VaMGj-wkAXbudhMCFjzg8saaI2PcmqAr7qEKkgxphrNURuOOueMBVCp6DMBCJvuC26L9C0FM8ILPHzP-27FQVxMPuFnBLF4oeq1Qnj2Ak5VPWP5aLshZHxXhIV9-6JiVORfeGg2" />        <input id="UserId" name="UserId" type="text" />
                                <input id="PromotionId" name="PromotionId" type="text" />
                                <input id="Stage" name="Stage" type="text" />
                                <input id="File" type="file" name="file" onchange="DataUpload()">
                            </form>
                            <p>
                                <a class="btn btn-dark" href="/Document/Index" translate>Back</a>
                            </p>
                        </div>
                    </div>
                    <!--END CONTENT-->
                </div>
            </div>
        </div>
    </div>
    <!--END CONTENT-->
</div>

<script src="~/Scripts/api/common.js"></script>
<script>
    var shopId = $("#ShopId").val();
    var fileId = $("#FileId").val();

    $(function () {
        $.commonGet("CommitFile/ShopCommitFileRecordSearch", { shopId: shopId, fileId: fileId }, function (data) {
            if (data) {
                $("#myPromotion").bootstrapTable({
                    data: data
                });
            }
        })
    })
</script>